@model Bank
<div class="modal fade" id="addBank" tabindex="-1" role="dialog"
     aria-labelledby="bankaddmodal" style="padding-right: 17px;">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header common-bg rounded-0">
                <h5 class="modal-title h4 color-white md-font"></h5>
                <button type="button" class="close color-white" data-dismiss="modal" aria-label="Close">
                    <span class="color-white" aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body">
                <form asp-action="Create" method="post">
                <table>
                    <tbody>
                        <tr>
                            <td class="sym-mod-lef text-left pb-2 pr-3"> <label for="" class="sm-font mb-0">AccountName</label><span class="pl-rgt pr-2" style="float: right;">:</span></td>
                            <td class="sym-mod-right pb-2"><input asp-for="accountName" type="text" class="input-height sm-font"></td>
                        </tr>
                        <tr>
                            <td class="sym-mod-lef text-left pb-2 pr-3"> <label for="" class="sm-font mb-0">BankName</label><span class="pl-rgt pr-2" style="float: right;">:</span></td>
                                <td class="sym-mod-right pb-2"><input asp-for="bankName" type="text" class="input-height sm-font"></td>
                        </tr>
                        <tr>
                            <td class="sym-mod-lef text-left pb-2 pr-3"> <label for="" class="sm-font mb-0">AccountNumber</label><span class="pl-rgt pr-2" style="float: right;">:</span></td>
                                <td class="sym-mod-right pb-2"><input asp-for="accountNumber" type="number" class="input-height sm-font"></td>
                        </tr>
                        <tr>
                            <td class="sym-mod-lef text-left pb-2 pr-3"> <label for="" class="sm-font mb-0">IFSCCode</label><span class="pl-rgt pr-2" style="float: right;">:</span></td>
                                <td class="sym-mod-right pb-2"><input asp-for="ifscCode" type="text" class="input-height sm-font"></td>
                        </tr>
                        <tr>
                            <td class="sym-mod-lef text-left pb-2 pr-3"> <label for="" class="sm-font mb-0">BranchName</label><span class="pl-rgt pr-2" style="float: right;">:</span></td>
                                <td class="sym-mod-right pb-2"><input asp-for="branchName" type="text" class="input-height sm-font"></td>
                        </tr>
                        <tr>
                            <td class="sym-mod-lef text-left pb-2 pr-3"> <label for="" class="sm-font mb-0">BankLogo</label><span class="pl-rgt pr-2" style="float: right;">:</span></td>
                            <td class="sym-mod-right pb-2">
                                    <select asp-items="ViewBag.bankLogo" asp-for="bankLogo">
                                        <option value="">Please Select</option>
                                </select>
                            </td>
                        </tr>

                    </tbody>
                </table>
                <div class="btn-sub text-center">
                    <button class="btn common-bg sm-font">Submit</button>
                </div>
                </form>
            </div>
        </div>
    </div>
</div>

